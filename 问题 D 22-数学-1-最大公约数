#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{ 
	//问题 D: 22-数学-1-最大公约数
 	int k;
 	scanf("%d",&k);
 	//k为样例个数
 	int ix=0;  
	for(ix=0;ix<k;ix++)
 	{
 		int a,b;
		scanf("%d%d",&a,&b);
		//a为区间左端，b为右端 
		int cnt1=b-b/2-b/3+b/6;
		int cnt2=(a-1)-(a-1)/2-(a-1)/3+(a-1)/6; 
		printf("%d\n",cnt1-cnt2);
	} 
	return 0;
}
 
