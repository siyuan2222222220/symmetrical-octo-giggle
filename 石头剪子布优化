#include<stdio.h>
#include<string.h>
int main()
{
	int n;
	scanf("%d",&n);
	char str1[20],str2[20];
	int i=0;
	for(i=0;i<n;i++)
	{
		scanf("%s",str1);
		scanf("%s",str2);
		if(str1[0]=='P'&&str2[0]=='R'||str1[0]=='R'&&str2[0]=='S'||str1[0]=='S'&&str2[0]=='P')
		{
			printf("Player1\n");
		}
		else if(str1[0]==str2[0])
		{
			printf("Tie\n");
		}
	}
	return 0;
}
