#include<stdio.h>
int main()
{
	int n;
	scanf("%d",&n);
	int age;
	int p1=0,p2=0,p3=0,p4=0;
	int i=0;
	for(i=0;i<n;i++)
	{
		scanf("%d",&age);
		if(age<=18)
		p1++;
		else if(age<=35)
		p2++;
		else if(age<=60)
		p3++;
		else 
		p4++;
	}
	printf("%.2lf%%\n%.2lf%%\n%.2lf%%\n%.2lf%%\n",100.0*p1/n,100.0*p2/n,100.0*p3/n,100.0*p4/n);
	return 0;
}
