#include<stdio.h>
#include<string.h>
int main()
{
	char str[260];
	scanf("%s",str);
	int len=strlen(str);
	int i=0;
	for(i=0;i<len;i++)
	{
		if(str[i]=='A')
		//printf("T");
		putchar('T');
		else if(str[i]=='T')
		printf("A");
		else if(str[i]=='C')
		printf("G");
		else if(str[i]=='G')
		printf("C");
	}
	printf("\n");
	return 0;
}
