//打印素数
#include<stdio.h>
#include<math.h>
int main()
{
	int i=0;
	for(i=101;i<=200;i+=2)
	{
		//偶数不是素数
		//判断i是否为素数
		//素数判断的规则
		//1.试除法
		int j=0;
		for(j=2;j<=sqrt(i);j++)
		{
				//j*j<=i
			if(i%j==0)
			{
				break;
			}
			if(j>sqrt(i))
			{
				printf("%d ",i); 
				 
			}		
		} 
	} 
	return 0;
} 
