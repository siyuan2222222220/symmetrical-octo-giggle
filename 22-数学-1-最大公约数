#include<stdio.h>
//问题 D: 22-数学-1-最大公约数
//1-2,1
//1-3,
//1-4,
//1-5,1,5
//1-6,
//1-7,1,5,7
//1-8,
//1-9,
//1-10,
//1-11,1,5,7,11
//1-12,
//1-13,1,5,7,11,13
//1-14,
//1-15,
//1-16,
//1-17,1,5,7,11,13,17
//1-18,
//1-19,1,5,7,11,13,17,19

int main()
{
	int k;
	scanf("%d",&k);
	int i=0;
	for(i=0;i<k;i++)
	{
		int a,b;
		scanf("%d%d",&a,&b);
		int cnt=0;
		int j=0;
		for(j=a;j<=b;j++)
		{
			if(j%2!=0&&j%3!=0)
			{
				cnt++;
			}
		}
		printf("%d\n",cnt);
	}
	
	return 0;
}	
