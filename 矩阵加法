#include<stdio.h>
int main()
{
	//B2104 矩阵加法
	int m,n;
	scanf("%d%d",&m,&n);
	int arr[m][n];
	int str[m][n];
	int i=0;
	for(i=0;i<m;i++)
	{
		int j=0;
		for(j=0;j<n;j++)
		{
			scanf("%d",&arr[i][j]);	
		}	
	}
	for(i=0;i<m;i++)
	{
		int j=0;
		for(j=0;j<n;j++)
		{
			scanf("%d",&str[i][j]);
			printf("%d ",str[i][j]+arr[i][j]);	
		}
		printf("\n");	
	}
	
	return 0;	
} 
