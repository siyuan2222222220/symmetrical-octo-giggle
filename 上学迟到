#include<stdio.h>
int main()
{
	int s,v;
	scanf("%d%d",&s,&v);
	//这道题有难度的
	//现在没多少时间了
	//明天再来想
	//先github和英语听力 
	
	//我回来了
	//首先，因为要垃圾分类，
	//所以其实每次都是从7:50开始的
	//然后我们接着来思考
	//题目给出了s,v
	//那么我们是不是可以算出来要花的时间
	//如果不能刚好整除，那么就应该
	//再减一分钟凑整
	//如果能整除，那么就直接减s/v就行
	//以上就是分支
	//接下来我们就要思考一个问题
	//那就是如何把所用掉的时间转化
	//为对应提前出门的时刻 
	///应该想到的是，每减少60分钟就应该
	//转化成减小了1个小时
	//又因为题目说可以提前一天之内，
	//也就是说，可能是上学的前一天
	//基于此，当分钟也就是s/v>=470时，就要用分支
	//该种情况hh就要等于23
	//其他则是正常计算
	//然后就是小时制60进1
	//operate
	//int minutes=0;
	//if(s%v==0)
	//{
	//	minutes=s/v;	
	//} 
	//else
	//minutes=s/v+1;
	//int hh=7;
	//int mm=50;
	//int i=0;
	//for(i=0;i<minutes;i++)
	///{
	//	if(minutes>470)
	//	{
	//		hh+=23;
	//	}
	//	if(mm==0)
	//	{
	//		hh--;
	//		mm=60;
	//	}
	//	mm--;
	//}
	//优化
	//因为小时相减实际上是非常麻烦的
	//所以干脆直接全都化成分钟
	//8:00就是480
	//花了10分钟捡垃圾就是470
	//因为有可能会是前一天出发
	//所以直接都加上24*60=1440
	//然后计算结果最后直接取余就行
	int minutes=0;
	if(s%v==0)
	{
		minutes=s/v;	
	} 
	else
	minutes=s/v+1;
	//time表示出发时间的分钟形式 
	int time=(470+24*60-minutes)%1440;
	//现在将time变成小时制
	int hh=time/60; 
	int mm=time%60;
	printf("%02d:%02d\n",hh,mm);
	return 0; 
}
