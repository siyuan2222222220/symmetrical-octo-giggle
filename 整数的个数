#include<stdio.h>
int main()
{
	// 整数的个数
	int k;
	scanf("%d",&k);
	int num;
	int cnt1=0,cnt5=0,cnt10=0;
	int i=0;
	for(i=0;i<k;i++)
	{
		scanf("%d",&num);
		if(num==1)
		cnt1++;
		else if(num==5)
		cnt5++;
		else if(num==10)
		cnt10++;
		else ;
	}
	printf("%d\n%d\n%d\n",cnt1,cnt5,cnt10);
	return 0;
}
