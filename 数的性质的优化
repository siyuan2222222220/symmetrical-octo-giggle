//数的性质优化
#include<stdio.h>
int main()
{
	int a,sum;
	scanf("%d",&a);
	int k[4]={0};
	if(a%2==0)
	{
		sum++;
	}
	if(a>4&&a<=12)
	{
		sum++;
	}
	if(sum==2)
	{
		k[0]=1;
	}
	if(sum==1||sum==2)
	{
		k[1]=1;	
	}
	if(sum==1)
	{
		k[2]=1;	
	}	
	if(sum==0)
	{
		k[3]=1;
	}
	int i=0;
	for(i=0;i<4;i++)
	{
		printf("%d ",k[i]);
	}
	printf("\n");
	return 0;
} 
