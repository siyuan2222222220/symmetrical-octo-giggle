#include<stdio.h>
int main()
{
	int n;
	//scanf("%d",&n);
	n=5;
	int k=0;
	for(k=0;k<n-2;k++)
	{
		int i=0;
		for(i=0;i<n-3-k;i++)
		{
			printf(" ");	
		}
		int j=0;
		for(j=0;j<1+2*k;j++)
		{
			printf("*");
		}
		printf("\n");
	}
	
	
	for(k=0;k<n-3;k++)
	{
		int i=0;
		for(i=0;i<n-4+k;i++)
		{
			printf(" ");	
		}
		int j=0;
		for(j=0;j<n-2-2*k;j++)
		{
			printf("*");
		}
		printf("\n");
	}
	
	
	
	return 0;
}
