#include<stdio.h>
int main()
{
	//优化的小鱼的航程
	//原来我是用了很多个分支把每一周的每一天分开
	//代码就很复杂
	//但是这里就很清晰了
	//我们来一一分析
	//首先输入x--周几,n--过了n天 
	int x,n;
	scanf("%d%d",&x,&n);
	//总共游的距离 
	int miles=0;
	//用循环天数，这个想法真的很好
	//大大简化了各种分支使用 
	int i=0;
	for(i=0;i<n;i++)
	{
		//如果不是周六和周日
		//就游250里 
		if(x!=6&&x!=7)
		{
			miles+=250;	
		}
		//当来到星期日，下一个就应该是星期一
		//周而复始 
		if(x==7)
		{
			x=1;	
		}
		//周几加1，第二天	
		else 
		x++;
	} 
	printf("%d\n",miles);
	return 0;
}
